// Configuration settings file format documentation can be found at:
// https://help.apple.com/xcode/#/dev745c5c974

// App Configuration
PRODUCT_NAME = BudgetHunter
PRODUCT_BUNDLE_IDENTIFIER = com.meneses.budgethunter
PRODUCT_VERSION = 1.0.0
MARKETING_VERSION = 1.0.0

// Deployment
IPHONEOS_DEPLOYMENT_TARGET = 15.0
TARGETED_DEVICE_FAMILY = 1,2

// Code Signing
CODE_SIGN_STYLE = Automatic
DEVELOPMENT_TEAM = 

// Swift
SWIFT_VERSION = 5.0

// Framework Integration
FRAMEWORK_SEARCH_PATHS = $(inherited) $(SRCROOT)/../composeApp/build/XCFrameworks/debug $(SRCROOT)/../composeApp/build/XCFrameworks/release
OTHER_LDFLAGS = $(inherited) -framework ComposeApp

// Asset Configuration
ASSETCATALOG_COMPILER_APPICON_NAME = AppIcon

// Preview Support
ENABLE_PREVIEWS = YES

// Run Path
LD_RUNPATH_SEARCH_PATHS = $(inherited) @executable_path/Frameworks